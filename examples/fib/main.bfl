malloc x,y,z,count;

@define MAX_COUNT 8

@incmacro intlib/intlib
:intlib/intlib:;
main {
    copy :MAX_COUNT: -> count;
    copy 0 -> x;
    copy 1 -> y;
    while(count) {
        copy x -> accumulator;
        :intlib/printint:;
        add x, y -> z;
        copy y -> x;
        copy z -> y;
        sub count, 1 -> count;
    (count)
    }
}